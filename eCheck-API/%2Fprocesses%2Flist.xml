<?xml version="1.0" encoding="UTF-8"?>
<con:resource xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:con="http://eviware.com/soapui/config" name="/processes/list" path="/processes/list" id="ee46a22e-fd52-456a-b15e-37c3c443d450">
  <con:settings id="846bb205-1aa5-4817-8f7e-b42f79ed3eaf">
    <con:setting id="ee46a22e-fd52-456a-b15e-37c3c443d450fileName">%2Fprocesses%2Flist</con:setting>
  </con:settings>
  <con:parameters/>
  <con:method name="searchProcesses" id="37c9a638-8273-4bee-9109-6e964a0c1e6c" method="GET">
    <con:description>The implementations must support criteria search by Process.description column, pagination and sort by Process.description.
Search list of processes by given criteria, page number, page size, sort column and sort order</con:description>
    <con:settings/>
    <con:parameters>
      <con:parameter>
        <con:name>criteria</con:name>
        <con:style>QUERY</con:style>
        <con:description>the criteria to search (wildcard match). If null then no criteria filter applied</con:description>
      </con:parameter>
      <con:parameter>
        <con:name>pageNumber</con:name>
        <con:style>QUERY</con:style>
        <con:dataType>integer</con:dataType>
        <con:description>the page 0 based indexing. Must be >=0 if present. Assume 0 by default if null</con:description>
      </con:parameter>
      <con:parameter>
        <con:name>pageSize</con:name>
        <con:style>QUERY</con:style>
        <con:dataType>integer</con:dataType>
        <con:description>the maximum number of results to return. Must be >0 if present. Assume Integer.MAX by default if null</con:description>
      </con:parameter>
      <con:parameter>
        <con:name>sortColumn</con:name>
        <con:style>QUERY</con:style>
        <con:description>the sort by column, if implementations doesn't support sorting by the provided column, implementations must return 400 with proper error message and error code</con:description>
      </con:parameter>
      <con:parameter>
        <con:name>sortOrder</con:name>
        <con:style>QUERY</con:style>
        <con:description>the sort order. Must be ASC or DESC if present. Assume ASC by default if null</con:description>
      </con:parameter>
    </con:parameters>
    <con:representation type="RESPONSE">
      <con:mediaType>application/json</con:mediaType>
      <con:status>200</con:status>
      <con:params/>
      <con:sampleContent>{
  "criteria" : "string",
  "pageNumber" : 0,
  "pageSize" : 0,
  "sortColumn" : "string",
  "sortOrder" : "ASC",
  "totalCount" : 1.5,
  "records" : [ {
    "createdBy" : "string",
    "createdAt" : "2015-07-20T15:49:04-07:00",
    "updatedBy" : "string",
    "updatedAt" : "2015-07-20T15:49:04-07:00",
    "id" : 1.5,
    "description" : "string"
  } ]
}</con:sampleContent>
    </con:representation>
    <con:representation type="RESPONSE">
      <con:mediaType>application/json</con:mediaType>
      <con:status>400</con:status>
      <con:params/>
      <con:sampleContent>{
  "code" : "string",
  "message" : "string"
}</con:sampleContent>
    </con:representation>
    <con:representation type="RESPONSE">
      <con:mediaType>application/json</con:mediaType>
      <con:status>401</con:status>
      <con:params/>
      <con:sampleContent>{
  "code" : "string",
  "message" : "string"
}</con:sampleContent>
    </con:representation>
    <con:representation type="RESPONSE">
      <con:mediaType>application/json</con:mediaType>
      <con:status>403</con:status>
      <con:params/>
      <con:sampleContent>{
  "code" : "string",
  "message" : "string"
}</con:sampleContent>
    </con:representation>
    <con:representation type="REQUEST">
      <con:mediaType>application/json</con:mediaType>
      <con:params/>
    </con:representation>
    <con:request name="Request 1" id="1aac369a-5b1f-450f-a853-91fd744d1c7a" mediaType="application/json">
      <con:settings/>
      <con:endpoint>https://localhost:8080</con:endpoint>
      <con:credentials>
        <con:selectedAuthProfile>No Authorization</con:selectedAuthProfile>
        <con:authType>No Authorization</con:authType>
      </con:credentials>
      <con:parameters/>
    </con:request>
  </con:method>
</con:resource>
