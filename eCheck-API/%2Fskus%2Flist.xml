<con:resource xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:con="http://eviware.com/soapui/config" name="/skus/list" path="/skus/list" id="2d97c1d2-d640-4474-9b5c-f4b05ff37f52">
  <con:settings id="f55ac83e-c713-4e19-8f6e-307929cc03db">
    <con:setting id="2d97c1d2-d640-4474-9b5c-f4b05ff37f52fileName">%2Fskus%2Flist</con:setting>
