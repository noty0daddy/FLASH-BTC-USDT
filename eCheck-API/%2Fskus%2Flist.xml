  <con:parameters/>
  <con:method name="searchSKUs" id="40d9e232-2f47-44df-884d-0802667a9aa5" method="GET">
    <con:description>The implementations must support criteria search by eCheck_SKU.sku_num column, pagination and sort by Process.description, eCheck_SKU.sku_num, eCheck_SKU.description.
Search list of SKUs by given criteria, page number, page size, sort column and sort order</con:description>
    <con:settings/>
    <con:parameters>
      <con:parameter>
        <con:name>criteria</con:name>
        <con:style>QUERY</con:style>
        <con:description>the criteria to search (wildcard match). If null then no criteria filter applied</con:description>
      </con:parameter>
      <con:parameter>
        <con:name>pageNumber</con:name>
        <con:style>QUERY</con:style>
        <con:dataType>integer</con:dataType>
        <con:description>the page 0 based indexing. Must be >=0 if present. Assume 0 by default if null</con:description>
      </con:parameter>
      <con:parameter>
        <con:name>pageSize</con:name>
        <con:style>QUERY</con:style>
        <con:dataType>integer</con:dataType>
        <con:description>the maximum number of results to return. Must be >0 if present. Assume Integer.MAX by default if null</con:description>
      </con:parameter>
      <con:parameter>
        <con:name>sortColumn</con:name>
        <con:style>QUERY</con:style>
        <con:description>the sort by column, if implementations doesn't support sorting by the provided column, implementations must return 400 with proper error message and error code</con:description>
      </con:parameter>
      <con:parameter>
        <con:name>sortOrder</con:name>
        <con:style>QUERY</con:style>
        <con:description>the sort order. Must be ASC or DESC if present. Assume ASC by default if null</con:description>
      </con:parameter>
    </con:parameters>
    <con:representation type="RESPONSE">
      <con:mediaType>application/json</con:mediaType>
      <con:status>200</con:status>
      <con:params/>
      <con:sampleContent>{
  "criteria" : "string",
  "pageNumber" : 0,
  "pageSize" : 0,
  "sortColumn" : "string",
  "sortOrder" : "ASC",
  "totalCount" : 1.5,
  "records" : [ {
    "createdBy" : "string",
    "createdAt" : "2015-07-20T15:49:04-07:00",
    "updatedBy" : "string",
    "updatedAt" : "2015-07-20T15:49:04-07:00",
    "id" : 1.5,
    "process" : {
      "createdBy" : "string",
      "createdAt" : "2015-07-20T15:49:04-07:00",
      "updatedBy" : "string",
      "updatedAt" : "2015-07-20T15:49:04-07:00",
      "id" : 1.5,
      "description" : "string"
    },
    "skuNum" : "string",
    "description" : "string"
  } ]
}</con:sampleContent>
    </con:representation>
    <con:representation type="RESPONSE">
      <con:mediaType>application/json</con:mediaType>
      <con:status>400</con:status>
      <con:params/>
      <con:sampleContent>{
  "code" : "string",
  "message" : "string"
}</con:sampleContent>
    </con:representation>
    <con:representation type="RESPONSE">
      <con:mediaType>application/json</con:mediaType>
